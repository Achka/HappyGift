@using System.Collections.Generic
@model List<HappyGift.Models.GiftViewModels.GiftViewModel>
@{
    ViewData["Title"] = "View";
}

<h2>Gifts history of orders</h2>

<div class="old-gifts">
    @for (int i = 0; i < Model.Count; i++)
    {
        <div> Gift № @(i+1):  </div>
        @foreach (var gift in Model[i].Services)
        {
            <div> @gift  </div>
        }
        <br>
    }
</div>

